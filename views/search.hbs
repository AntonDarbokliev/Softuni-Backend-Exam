<section class="search">
            <h1>Search</h1>

            <form class="search-form" action="#" method="GET">
                <input type="text" class="search-name" name="searchName" placeholder="Search Name...">
                <input type="text" class="search-type" name="searchType" placeholder="Search Type...">
                <button type="submit" class="btn-search">Search</button>
            </form>
                {{#if electronics}}
                {{#each electronics}}
                {{> searchCard}}
                {{/each}}
                {{else}}
            <!--If there are no matches-->
            <div class="no-match">
                <p>No match was found!</p>
            </div>
                {{/if}}
        </section>